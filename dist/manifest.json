{
    "manifest_version": 3,
    "name": "Argus",
    "version": "1.0.0",
    "description": "Trial ext",
    "icons": {
      "48": "icon1.png",
      "128": "icon1.png"
    },
    "permissions": ["activeTab","storage","tabs","webNavigation","scripting"],
    "host_permissions": [
      "https://mail.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
 
  
    
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
              
        "run_at":"document_end"
        
        
      }],
    "action":{ "default_title": " AI Extension", "default_popup": "popup.html" }
  }
